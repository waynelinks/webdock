{
  "name": "@webdock/the-greeter",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "http://127.0.0.1:54873",
    "access": "restricted"
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "duck-interface": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "jest": "*",
    "ts-jest": "*",
    "ts-node-dev": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc",
    "lint": "tslint --exclude './{coverage,dist,node_modules}/**/*' './**/*.{ts,js}'",
    "lint-fix": "tslint --exclude './{coverage,dist,node_modules}/**/*' './**/*.{ts,js}' --fix",
    "test": "jest",
    "test-coverage": "rm -rf ./coverage && jest --coverage",
    "test-watch": "jest --watchAll",
    "unit-tests": "jest ./tests/unit-tests",
    "integration-tests": "jest ./tests/integration-tests"
  }
}
