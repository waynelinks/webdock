{
  "name": "@webdock/the-web-cloud-tester",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@types/jest": "*",
    "@types/node": "*",
    "@types/node-fetch": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "jest": "*",
    "node-fetch": "*",
    "ts-jest": "*",
    "typescript": "*"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint './**/*.{js,jsx,ts,tsx}' && tsc --noEmit",
    "lint-fix": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
    "test": "jest",
    "test-coverage": "rm -rf ./coverage && jest --coverage",
    "test-watch": "jest --watchAll",
    "unit-tests": "jest ./tests/unit-tests",
    "integration-tests": "jest ./tests/integration-tests",
    "target-api-tests": "jest ./tests/target-api-tests",
    "target-smoke-tests": "jest ./tests/target-smoke-tests"
  }
}
