{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "typedef": [
      true,
      "array-destructuring",
      "arrow-call-signature",
      "arrow-parameter",
      "call-signature",
      "member-variable-declaration",
      "object-destructuring",
      "parameter",
      "property-declaration",
      "variable-declaration"
    ]
  },
  "jsRules": true
}
